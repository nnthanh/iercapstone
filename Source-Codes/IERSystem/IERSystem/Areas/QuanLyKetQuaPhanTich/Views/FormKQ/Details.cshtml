@model IERSystem.Areas.Administrator.Models.FormKQ

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Administrator/Views/Shared/_Layout.cshtml";
}

<h2>Chi tiết kết phiếu kết quả</h2>

<div>
    <div class="clearfix">
    </div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <dl class="dl-horizontal">
                    <dt>
                        <label class="control-label col-md-2 col-sm-2 col-xs-12" for="first-name">
                            Nơi lấy mẫu
                        </label>
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.NoiLayMau)
                    </dd>

                    <dt>
                        <label class="control-label col-md-2 col-sm-2 col-xs-12" for="first-name">
                            Ngày gửi mẫu
                        </label>
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.NgayGuiMau)
                    </dd>

                    <dt>
                        <label class="control-label col-md-2 col-sm-2 col-xs-12" for="first-name">
                            Địa chỉ
                        </label>
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.DiaChi)
                    </dd>

                    <dt>
                        <label class="control-label col-md-2 col-sm-2 col-xs-12" for="first-name">
                            Loại mẫu
                        </label>
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.LoaiMau)
                    </dd>

                    <dt>
                        <label class="control-label col-md-2 col-sm-2 col-xs-12" for="first-name">
                            Vị trí lấy mẫu
                        </label>
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.ViTriLayMau)
                    </dd>

                </dl>
            </div>
            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">
                        <div class="x_title">
                            <h2>
                                Phiếu kết quả
                            </h2>
                            <ul class="nav navbar-right panel_toolbox">
                                <li>
                                    <a class="collapse-link">
                                        <i class="fa fa-chevron-up"></i>
                                    </a>
                                </li>
                                <li>
                                    <a class="close-link">
                                        <i class="fa fa-close"></i>
                                    </a>
                                </li>
                            </ul>
                            <div class="clearfix"></div>
                        </div>
                        <div class="x_content" style="display:block;">
                            <table class="table table-striped">
                                <thead>
                                    <tr>
                                        <th>Số thứ tự</th>
                                        <th>Chỉ tiêu</th>
                                        <th>Đơn vị</th>
                                        <th>Giá trị</th>
                                        <th>Phương pháp phân tích</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<p class="mid_center">
    <button class="btn btn-warning" data-toggle="tooltip" title="Chỉnh sửa" onclick="location.href='@Url.Action("Edit", "FormKQ", new { id = Model.Id })'"><span class="fa fa-edit"></span></button>
    <button class="btn btn-success" data-toggle="tooltip" title="Quay lại" onclick="location.href='@Url.Action("Index", "FormKQ", new { id = Model.Id })'"><span class="fa fa-arrow-left"></span></button>
</p>