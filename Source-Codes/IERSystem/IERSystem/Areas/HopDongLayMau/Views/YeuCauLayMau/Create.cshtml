                            styling: 'bootstrap3'
                        });
                        setTimeout(function () {
                            window.location.href = '@Url.Action("Index", "YeuCauLayMau")'
                        }, 1500);
                    } else {
                        new PNotify({
                            title: 'Thất bại',
                            text: 'kiểm tra lại thông tin về hợp đồng của bạn rồi thử lại.',
                            type: 'error',
                            styling: 'bootstrap3'
                        });
                    }
                },
                dataType: 'json',
                contentType: 'application/json; charset=utf-8'
            });
        });
