@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")


<!-- validator -->
<script>
    // initialize the validator function
    validator.message.date = 'Không phải ngày thật';

     //validate a field on "blur" event, a 'select' on 'change' event & a '.reuired' classed multifield on 'keyup':
    $('form')
      .on('blur', 'input[required], input.optional, select.required', validator.checkField)
      .on('change', 'select.required', validator.checkField)
      .on('keypress', 'input[required][pattern]', validator.keypress);

    $('.multi.required').on('keyup blur', 'input', function () {
       validator.checkField.apply($(this).siblings().last()[0]);
    });

    function turnBack() {
        new PNotify({
            title: 'Chuyển trang',
            text: 'bạn đang được chuyển sang trang chủ.',
            type: 'success',
            styling: 'bootstrap3'
        });
        setTimeout(function () {
            window.location.href = '@Url.Action("Index", "YeuCauLayMau")'
        }, 1500);
        
    }
</script>
<!-- /validator -->

    <!--nthoang-PhieuYeuCauThuNghiemView: Editable Table-->
    <!--nthoang-Cac logic lien quan den Bang Mau duoc code tai day-->
    
<script>
    $(document).ready(
        function () {
        $('#NgayLayMau').daterangepicker({
            singleDatePicker: true,
            calender_style: "picker_1"
        }, function (start, end, label) {
            console.log(start.toISOString(), end.toISOString(), label);
        });
        
            
    });
</script>

<script>
    $(document).ready(function () {
        var $Global = [];
        var $CacheChiTieu = {};
        var $SelectedCT_Id = [];
        var chitieu_selectbox = $("#chitieu-yeucau");

        $.ajax({
            type: 'GET',
            url: ' @Url.Action("GetNhomChiTieu", "API", new { area = "BaoGiaChiTieu" })',
            success: function (res) {
                var loaimau_selectbox = $("#loai-chitieu");
                loaimau_selectbox.append("<option></option>");
                res.forEach(function (item) {
                    loaimau_selectbox.append("<option>" + item.TenNhom + "</option>");
                });
            },
            dataType: 'json',
            contentType: 'application/json; charset=utf-8'
        });

        $('#loai-chitieu').change(function () {
            //nthoang: AJAX call ChiTieuMau suggestion list
            var formData2 = {
                TenNhom: $("#loai-chitieu").val()
            };
            if (!(formData2.TenNhom in $CacheChiTieu)) {
                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("FindChiTieu", "API", new { area = "BaoGiaChiTieu" })',
                    data: JSON.stringify(formData2),
                    success: function (res) {
                        chitieu_selectbox.empty();
                        $CacheChiTieu[formData2.TenNhom] = res;
                        res.forEach(function (item) {
                            chitieu_selectbox.append("<option>" + item.TenChiTieu + "</option>");
                        });
                    },
                    dataType: 'json',
                    contentType: 'application/json; charset=utf-8'
                });
            } else {
                chitieu_selectbox.empty();
                var cached_ct = $CacheChiTieu[formData2.TenNhom];
                cached_ct.forEach(function (item) {
                    chitieu_selectbox.append("<option>" + item.TenChiTieu + "</option>");
                });
            }
        });

        $('#themMauModal').on('hidden.bs.modal', function () {
            $(this).find("input,textarea,select").val('').end();
            $(this).find(".select2-selection__rendered").empty();
        });

        function Delete() {
            var par = $(this).parent().parent(); //tr
            par.remove();
        }

        function OnRowIn() {
            var deleteCell = $(this).children().last();
            deleteCell.html("<a id='deleteRowBtn' href='#'><span class='glyphicon glyphicon-remove'></span></a>");
            $('#deleteRowBtn').bind("click", Delete);
        }

        function OnRowOut() {
            $(this).children().last().html('');
        }

        $("#saveMauBtn").bind("click", function () {

            var tabledata = $("#sampledata-table tbody");
            var soluong = $("#so-luong").val();
            var donvi = $("#don-vi").val();
            var chitieuyeucaus = $(".select2-selection__choice").toArray().map(function (e) {
                return {
                    TenChiTieu: e.innerText.substr(1),
                    NhomChiTieu: $("#loai-chitieu").val()
                };
            });
            $Global.push({
                SoLuong: soluong,
                DonVi: donvi,
                ChiTieuYeuCaus: chitieuyeucaus
            });



            if ($('#vitri-laymau').val() && $('#mota-mau').val && $('#chitieu-yeucau').val()) {
                tabledata.append(
                "<tr>" +
                //"<td></td>" +
                "<td>" + $("#ma-mau").val() + "</td>" +
                "<td>" + $("#kh-mau").val() + "</td>" +
                "<td>" + $("#vitri-laymau").val() + "</td>" +
                "<td>" + soluong + " " + donvi + "</td>" +
                "<td>" + $("#mota-mau").val() + "</td>" +
                "<td>" + chitieuyeucaus.map(function (e) { return e.TenChiTieu }).join(", ") + "</td>" +
                "<td></td>" +
                "</tr>");
                //hide modal
                $('#themMauModal').modal('toggle');
            }
            else {
                new PNotify({
                    title: 'Thêm mẫu thất bại',
                    text: 'nhập đầy đủ các thông tin của mẫu rồi thử lại.',
                    type: 'error',
                    styling: 'bootstrap3'
                });
            }
            //On Row Hover
            tabledata.children().last().hover(OnRowIn, OnRowOut);

        });

        $("#submitBtn").bind("click", function () {
            function getFormData() {
                var res = {
                    TenKhachHang: $("#TenKhachHang").val(),
                    MaSoThue: $("#MaSoThue").val(),
                    TenDaiDien: $("#TenDaiDien").val(),
                    SoDienThoai: $("#SoDienThoai").val(),
                    SoFax: $("#SoFax").val(),
                    DiaChiLayMau: $("#DiaChiLayMau").val(),
                    DiaChiKhachHang: $("#DiaChiKhachHang").val(),
                    NgayLayMau: $("#NgayLayMau").val(),
                    NgayHenTraKQ: $("#NgayHenTraKQ").val().split(" ")[0],
                    PhuongPhapLayMau: $("#PhuongPhapLayMau").val(),
                    TenTieuChuanDoiChieu: $("#TenTieuChuanDoiChieu").val(),
                    PhiThiNghiemTamTinh: $("#PhiThiNghiemTamTinh").val(),
                    TienKhachHangTraTruoc: $("#TienKhachHangTraTruoc").val(),
                    KhachHangMoi: document.getElementById('isNewKH').checked
                };
                var sampledata = $("#sampledata-table tbody tr").map(function (i, elem) {
                    var rowdata = $(elem).find("td").map(function () { return $(this).text(); }).get();
                    //alert(rowdata);
                    return {
                        MaMauKH: rowdata[0],
                        //nthoang: Get the first token of the string splitted by ':' character of Kí Hiệu Mẫu
                        KiHieuMau: rowdata[1].split(":")[0],
                        ViTriLayMau: rowdata[2],
                        SoLuong:  $Global[i].SoLuong,
                        DonVi:  $Global[i].DonVi,
                        MoTaMau: rowdata[4],
                        ChiTieuPhanTiches: $Global[i].ChiTieuYeuCaus
                    }
                }).get();
                res.MauLayHienTruongs = sampledata;
                console.log(res);
                return res;
            }
            var formData = getFormData();
            $.ajax({
                type: 'POST',
                url: ' @Url.Action("Create", "API")',
                data: JSON.stringify(formData),
                success: function (res) {
